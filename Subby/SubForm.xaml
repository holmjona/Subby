<Window x:Class="Subby.SubForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Subby"
        mc:Ignorable="d"
        Title="This is the Sub Form" Height="100" Width="200" WindowStyle="None">
    <Grid>
        <TextBlock HorizontalAlignment="Left" Margin="41,25,0,0" TextWrapping="Wrap" Text="This is the Sub form" VerticalAlignment="Top"/>
        <Button x:Name="btnClose" Content="Close Me" HorizontalAlignment="Left" Margin="10,46,0,0" VerticalAlignment="Top" Width="75" Click="btnClose_Click"/>
        <Button x:Name="btnClose2" Content="Close Us" HorizontalAlignment="Left" Margin="100,46,0,0" VerticalAlignment="Top" Width="75" Click="btnClose2_Click"/>

    </Grid>
</Window>
